{
  "urls": {
    "app": "http://photo.garden",
    "photoServer": "http://localhost:3010"
  },
  "serviceUrls": {
    "photos": "http://api.photo.garden"
  },
  "queue": {
    "host": "queue",
    "port": 4222
  },
  "databases": {
    "garden": {
      "host": "db",
      "port": 5432,
      "user": "the_gardener",
      "password": "mysecretpassword",
      "database": "photo_garden"
    },
    "unleash": {
      "host": "db",
      "port": 5432,
      "user": "unleash",
      "password": "mysecretpassword",
      "database": "unleash"
    }
  },
  "gateway": {
    "port": 3000,
    "secret": "ThisIsNotSecret",
    "cookieName": "gardenSession"
  },
  "providers": {
    "googleDrive": {
      "validPhotos": ["image/jpeg"],
      "clientRedirectUri": "http://api.photo.garden/google-drive/auth/finish",
      "scopes": ["https://www.googleapis.com/auth/drive.readonly", "email"]
    },
    "dropbox": {
      "clientRedirectUri": "http://api.photo.garden/dropbox/auth/finish"
    },
    "instagram": {
      "clientRedirectUri": "http://api.photo.garden/instagram/auth/finish"
    }
  },
  "apis": {
    "photos": {
      "port": 3000
    }
  },
  "images": {
    "path": "/usr/src/photos"
  }
}
